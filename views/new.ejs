<%- include('./partials/header') %>
<div class="container">
    <div class="row">
        <h1 style="text-align: center;">Add New Project</h1>
        <div style="width: 30%; margin: 25px auto;">
            <form action="/campgrounds" method="POST">
                <div class="form-group">
                    <input class="form-control" type="text" name="name" placeholder="Enter name">
                </div>
                <div class="form-group">
                    <input class="form-control" type="text" name="image" placeholder="Image url">
                </div>
                <div class="form-group">
                    <textarea class="form-control" type="text" name="desc" placeholder="Enter description"></textarea>
                </div>
                <div class="form-group">
                    <button class="btn btn-lg btn btn-default btn-block" type="submit">Submit!</button>
                </div>
                
            </form>
            <a href="/">Go Back</a>

        </div>
    </div>
</div>
<%- include('./partials/footer') %>
